@model Enterprise.Web.MPA.Models.Product.ProductCategoryCardModel

<a>
    <div class="col-md-2 product-category-card">
        <div class="ibox">
            <div class="ibox-content product-box">
                <div class="product-imitation">
                    <img class="product-category-image" src="~/images/icons/productimage.png">
                </div>
                <div class="product-desc">
                    <a href="#" class="product-name text-center">@Model.CategoryName</a>
                </div>
            </div>
        </div>
    </div>
</a>